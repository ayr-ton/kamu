{
  "name": "library-ui",
  "private": true,
  "scripts": {
    "build:clean": "rm -rf ./dist/* && rm -rf ./.tmp/* && mkdir ./dist/scripts",
    "build:env": "node ./scripts/environment.js",
    "build:wiredep": "wiredep -s ./app/index.html",
    "build:minify": "usemin ./app/index.html --dest ./dist/ --output ./dist/index.html",
    "build:copy": "cp -rf ./app/{images,views} ./dist/ && cp ./app/*.{ico,txt} ./dist/",
    "build": "npm run build:clean && npm run build:env && npm run build:wiredep && npm run build:minify && npm run build:copy",
    "test": "npm run build && karma start test/karma.conf.js --single-run",
    "postinstall": "bower install && npm run build",
    "start": "node ./web"
  },
  "dependencies": {
    "compression": "1.5.2",
    "connect": "^2.14.3",
    "express": "~4.13.1",
    "express-session": "^1.11.3",
    "morgan": "~1.6.1",
    "passport": "^0.2.0",
    "passport-saml": "git://github.com/bergie/passport-saml.git",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "serve-favicon": "~2.3.0",
    "ejs": "~2.3.3"
  },
  "devDependencies": {
    "bower": "1.4.1",
    "coffee-script": "^1.9.3",
    "debug": "~2.2.0",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "~0.1.0",
    "karma-phantomjs-launcher": "*",
    "ng-annotate": "^1.0.2",
    "protractor": "2.1.0",
    "usemin-cli": "0.0.7",
    "wiredep": "^2.2.2"
  },
  "engines": {
    "node": "0.12.7",
    "npm": "2.13.5"
  }
}
