{
  "name": "library-ui",
  "private": true,
  "scripts": {
    "jshint": "jshint ./app/scripts/**/*.js ./test/**/*.js --config ./.jshintrc",
    "build:clean": "rm -rf ./dist/* && rm -rf ./.tmp/* && mkdir -p ./dist && mkdir -p ./dist/scripts",
    "build:env": "node ./scripts/environment.js",
    "build:wiredep": "wiredep -s ./app/index.html",
    "build:minify": "usemin ./app/index.html --dest ./dist/ --output ./dist/index.html --rmlr true",
    "build:copy": "cp -rf ./app/images ./dist && cp -rf ./app/views ./dist && cp -rf ./app/templates ./dist && cp ./app/404.html ./dist && cp ./app/error.html ./dist && cp ./app/*.ico ./dist && cp ./app/*.txt ./dist/",
    "build": "npm run build:clean && npm run jshint && npm run build:env && npm run build:wiredep && npm run build:minify && npm run build:copy",
    "test:protractor": "protractor test/protractor.conf.js",
    "test:acceptance": "./scripts/acceptance.sh",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test:watch": "watch 'npm run test:karma' ./app ./test",
    "test": "npm run test:karma",
    "livereload": "live-reload --port 9091 ./app",
    "start": "parallelshell 'node ./web' 'npm run livereload'",
    "postinstall": "bower install && npm run build"
  },
  "dependencies": {
    "angular-md5": "^0.1.8",
    "body-parser": "~1.13.2",
    "compression": "1.5.2",
    "connect": "^2.14.3",
    "cookie-parser": "~1.3.5",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "express-session": "^1.11.3",
    "morgan": "~1.6.1",
    "passport": "^0.2.0",
    "passport-saml": "git://github.com/bergie/passport-saml.git",
    "request": "^2.63.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "bower": "1.4.1",
    "coffee-script": "^1.9.3",
    "debug": "~2.2.0",
    "jshint": "^2.8.0",
    "karma-coverage": "^0.5.2",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-jasmine": "~0.1.0",
    "karma-phantomjs-launcher": "*",
    "karma-spec-reporter": "0.0.20",
    "karma-wiredep": "^1.0.1",
    "live-reload": "^1.1.0",
    "ng-annotate": "^1.0.2",
    "parallelshell": "^2.0.0",
    "passport-local": "^1.0.0",
    "protractor": "2.1.0",
    "request": "^2.64.0",
    "usemin-cli": "0.0.7",
    "watch": "^0.16.0",
    "wiredep": "^2.2.2"
  },
  "engines": {
    "node": "0.12.7",
    "npm": "2.13.5"
  }
}
