{
  "name": "library-ui",
  "private": true,
  "scripts": {
    "build:clean": "rm -rf ./dist/* && rm -rf ./.tmp/* && mkdir ./dist/scripts",
    "build:env": "node ./scripts/environment.js",
    "build:wiredep": "wiredep -s ./app/index.html",
    "build:minify": "usemin ./app/index.html --dest ./dist/ --output ./dist/index.html",
    "build:copy": "cp -rf ./app/{images,views} ./dist/ && cp ./app/*.{ico,txt} ./dist/",
    "build": "npm run build:clean && npm run build:env && npm run build:wiredep && npm run build:minify && npm run build:copy",
    "test": "npm run build && karma start test/karma.conf.js --single-run",
    "postinstall": "bower install && npm run build",
    "start": "node ./web"
  },
  "dependencies": {
    "bower": "1.4.1",
    "compression": "1.5.2",
    "connect": "^2.14.3",
    "express": "~4.13.1",
    "express-session": "^1.11.3",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "morgan": "^1.6.0",
    "passport": "^0.2.0",
    "passport-saml": "git://github.com/bergie/passport-saml.git",
    "protractor": "2.1.0"
  },
  "devDependencies": {
    "coffee-script": "^1.9.3",
    "grunt-angular-templates": "^0.5.7",
    "grunt-autoprefixer": "^2.0.0",
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.12.0",
    "grunt-contrib-htmlmin": "^0.4.0",
    "grunt-contrib-imagemin": "^0.9.2",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-filerev": "^2.1.2",
    "grunt-google-cdn": "^0.4.3",
    "grunt-karma": "*",
    "grunt-karma-coveralls": "^2.5.4",
    "grunt-newer": "^1.1.0",
    "grunt-ng-annotate": "^0.9.2",
    "grunt-ng-constant": "^1.1.0",
    "grunt-protractor-runner": "^2.1.0",
    "grunt-svgmin": "^2.0.0",
    "grunt-usemin": "^3.0.0",
    "grunt-wiredep": "^2.0.0",
    "jit-grunt": "^0.9.1",
    "jshint-stylish": "^1.0.0",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "~0.1.0",
    "karma-phantomjs-launcher": "*",
    "time-grunt": "^1.0.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "ng-annotate": "^1.0.2",
    "time-grunt": "^1.0.0",
    "wiredep": "^2.2.2"
  },
  "engines": {
    "node": "0.12.7",
    "npm": "2.13.5"
  }
}
